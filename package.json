{
  "name": "antlr4-c3",
  "version": "3.2.3",
  "description": "A code completion core implementation for ANTLR4 based parsers",
  "author": "Mike Lischke",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mike-lischke/antlr4-c3"
  },
  "keywords": [
    "ANTLR4",
    "code completion",
    "auto completion",
    "grammar",
    "parser"
  ],
  "type": "module",
  "main": "./lib/index.js",
  "scripts": {
    "build": "tsc",
    "mt": "tsc test.ts --project tsconfig.json",
    "prepublishOnly": "npm run generate && npm run test",
    "test": "jest --testMatch [ \"**/tests/**/*.spec.ts\" ] --no-coverage --watchAll=false --max-worker=1",
    "test-ci": "jest --testMatch [ \"**/tests/**/*.spec.ts\" ] --no-coverage --watchAll=false --silent",
    "test-coverage": "jest  --testMatch [ \"**/tests/**/*.spec.ts\" ] --coverage --silent",
    "generate": "antlr4ng -Dlanguage=TypeScript tests/FlinkSqlParser.g4 tests/FlinkSqlLexer.g4 tests/CPP14.g4 tests/Expr.g4 tests/Whitebox.g4 -no-listener -no-visitor -o tests/generated -Xexact-output-dir",
    "eslint": "eslint ."
  },
  "devDependencies": {
    "@babel/core": "^7.22.20",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-typescript": "^7.23.0",
    "@types/jest": "29.5.4",
    "@types/node": "20.5.9",
    "@types/unicode-properties": "1.3.0",
    "@typescript-eslint/eslint-plugin": "6.6.0",
    "@typescript-eslint/parser": "6.6.0",
    "antlr4ng-cli": "1.0.0",
    "babel-jest": "^29.7.0",
    "eslint": "8.48.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jsdoc": "46.5.1",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "jest": "29.6.4",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "antlr4ng": "^1.0.7"
  }
}
